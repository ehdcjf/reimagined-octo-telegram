{
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
                "project": "./tsconfig.json"
        },
        "env": {
                "node": true,
                "es2022": true
        },
        "extends": ["plugin:@typescript-eslint/recommended", "plugin:prettier/recommended"],
        "rules": {
                "semi": ["error", "always"],
                "prettier/prettier": 0,
                "@typescript-eslint/no-explicit-any": 0
        }
}
